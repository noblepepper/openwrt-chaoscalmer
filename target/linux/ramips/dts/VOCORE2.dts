/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
	compatible = "VoCore2", "mediatek,mt7628an-soc";
	model = "VoCore2";


	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	gpio-leds {
		compatible = "gpio-leds";

		status {
			label = "VoCore2:pink:status";
			gpios = <&gpio1 12 0>;
		};
	};

	chosen {
		bootargs = "console=ttyS2,115200";
	};


	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c";
				ralink,function = "gpio";
			};
			agpio {
				ralink,group = "agpio";
				ralink,function = "uart2";
			};
		};
	};
	
	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "en25q64";
				reg = <0 0>;
				linux,modalias = "m25p80", "en25q64";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xfb0000>;
				};
			};

		};
		i2s: i2s@a00 {
			compatible = "mediatek,mt7628-i2s";
			status = "okay";
		};
		uart1@d00 {
			status = "okay";
		};
		uart2@e00 {
			status = "okay";
		};
		gdma@2800 {
			compatible = "mediatek,mt7628-gdma";
			status = "okay";
		};
	};

	i2c-gpio {
		compatible = "i2c-gpio";

		gpios = <&gpio0 5 0 &gpio0 4 0>;
		i2c-gpio,delay-us = <5>;

		codec: wm8960@1a {
			compatible = "wm8960";
			model = "wm8960";
			reg = <0x1a>;
		};
        };

	audio-card {
		compatible = "mediatek,mt7628-audio";

		i2s-node = <&i2s>;
		codec-node = <&codec>;
		codec-dai-name = "wm8960-hifi";
	};
};
