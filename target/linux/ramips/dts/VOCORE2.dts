/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
	compatible = "VoCore2", "mediatek,mt7628an-soc";
	model = "VoCore2";


	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	gpio-leds {
		compatible = "gpio-leds";

		wifiled {
			label = "VoCore2:pink:wifi";
			gpios = <&gpio1 12 0>;
		};
		ethernetled {
			label = "VoCore2:green:eth0";
			gpios = <&gpio0 18 0>;
		};
	};

	chosen {
		bootargs = "console=ttyS2,115200";
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio0 {
				ralink,group = "gpio";
				ralink,function = "refclk";
			};
			i2c {
				ralink,group = "i2c";
				ralink,function = "i2c";
			};
			i2s {
				ralink,group = "i2s";
				ralink,function = "i2s";
			};
			pwm1 {
				ralink,group = "pwm1";
				ralink,function = "gpio";
			};
			uart2 {
				ralink,group = "uart2";
				ralink,function = "gpio";
			};
			sdmode {
				ralink,group = "sdmode";
				ralink,function = "sdxc";
			};
			wdt {
				ralink,group = "wdt";
				ralink,function = "gpio";
			};
			refclk {
				ralink,group = "refclk";
				ralink,function = "gpio";
			};
			agpio {
				ralink,group = "agpio";
				ralink,function = "uart2";
			};
			led4 {
				ralink,group = "p4led_an";
				ralink,function = "gpio";
			};
			led3 {
				ralink,group = "p3led_an";
				ralink,function = "gpio";
			};
			led2 {
				ralink,group = "p2led_an";
				ralink,function = "gpio";
			};
			led1 {
				ralink,group = "p1led_an";
				ralink,function = "gpio";
			};
			led0 {
				ralink,group = "p0led_an";
				ralink,function = "gpio";
			};
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio19 {
			gpio-export,name = "gpio19";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio0 19 0>;
		};

		gpio20 {
			gpio-export,name = "gpio20";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio0 20 0>;
		};

		gpio21 {
			gpio-export,name = "gpio21";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio0 21 0>;
		};

		gpio37 {
			gpio-export,name = "gpio37";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 5 0>;
		};

		gpio38 {
			gpio-export,name = "gpio38";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 6 0>;
		};

		gpio39 {
			gpio-export,name = "gpio39";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 7 0>;
		};

		gpio40 {
			gpio-export,name = "gpio40";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 8 0>;
		};

		gpio41 {
			gpio-export,name = "gpio41";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 9 0>;
		};

		gpio42 {
			gpio-export,name = "gpio42";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 10 0>;
		};

		gpio43 {
			gpio-export,name = "gpio19";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 11 0>;
		};
	};
	
	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "en25q64";
				reg = <0 0>;
				linux,modalias = "m25p80", "en25q64";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xfb0000>;
				};
			};

		};
		i2s: i2s@a00 {
			compatible = "mediatek,mt7628-i2s";
			status = "okay";
		};
		uart1@d00 {
			status = "okay";
		};
		uart2@e00 {
			status = "okay";
		};
		gdma@2800 {
			compatible = "mediatek,mt7628-gdma";
			status = "okay";
		};
	};

	i2c-gpio {
		compatible = "i2c-gpio";

		gpios = <&gpio0 5 0 &gpio0 4 0>;
		i2c-gpio,delay-us = <5>;

		codec: es8388@10 {
			compatible = "everest,es8388";
			model = "es8388";
			reg = <0x10>;
		};
        };

	audio-card {
		compatible = "mediatek,mt7628-audio";

		i2s-node = <&i2s>;
		codec-node = <&codec>;
		codec-dai-name = "es8388-hifi";
	};
};
